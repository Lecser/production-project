    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": true
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
